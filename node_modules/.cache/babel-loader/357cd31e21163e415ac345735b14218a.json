       // var chess1 = new Chess();\n       \n        // this.game.history().forEach(move => {\n        // console.log(\"Ovde sam MOVE: \" + move);\n        // chess1.move(move);\n        // this.board.set({fen: chess1.fen()})\n        // this.loadPosition() // uvek ide kad se menja pozicija\n        // });\n\n        console.log(moves)\n     },\n     provideHistory() {\n        console.log(this.game.history())\n     },\n     makeMove() {\n      this.game.move('e4')\n      this.board.set({fen: this.game.fen()})\n      this.loadPosition()\n     }\n  },\n  created() {\n    bus.$on('undo', () => {\n      this.undo()\n    }),\n    bus.$on('loadPgnPedja', () => {\n      this.loadPgnPedja()\n    }),\n    bus.$on('loadFenPedja', (fen) => {\n      this.loadFenPedja(fen)\n    }),\n    bus.$on('makeMove', () => {\n      this.makeMove()\n    })\n  }\n\n}\n</script>\n"],"mappings":";;;;;;AAEA,SAAAA,UAAA;AACA,OAAAC,KAAA;AACA,OAAAC,GAAA;AAEA;EACAC,IAAA;EACAC,OAAA,EAAAJ,UAAA;EACAK,OAAA;IACAC,YAAA,WAAAA,aAAAC,GAAA;MACA,KAAAC,IAAA,CAAAC,IAAA,CAAAF,GAAA;MACA,KAAAG,KAAA,CAAAC,GAAA;QAAAJ,GAAA,OAAAC,IAAA,CAAAD,GAAA;MAAA;MACAK,OAAA,CAAAC,GAAA,CAAAN,GAAA;MACA,KAAAO,YAAA;IACA;IACAC,IAAA,WAAAA,KAAA;MACA,KAAAP,IAAA,CAAAO,IAAA;MACA,KAAAL,KAAA,CAAAC,GAAA;QAAAJ,GAAA,OAAAC,IAAA,CAAAD,GAAA;MAAA;MACA,KAAAO,YAAA;IACA;IACAE,YAAA,WAAAA,aAAA;MACA,IAAAC,GAAA,IACA,eACA,cACA,uBACA,eACA,wBACA,8BACA,gBACA,eACA,oBACA;MAEA,KAAAT,IAAA,CAAAU,QAAA,CAAAD,GAAA,CAAAE,IAAA;MAEA,IAAAC,KAAA,QAAAZ,IAAA,CAAAa,OAAA;MACAT,OAAA,CAAAC,GAAA,0BAAAL,IAAA,CAAAa,OAAA;MAEA,IAAAC,KAAA,QAAAd,IAAA,CAAAe,MAAA;MACAX,OAAA,CAAAC,GAAA,6BAAAS,KAAA;MAEAV,OAAA,CAAAC,GAAA,4BAAAL,IAAA,CAAAe,MAAA;MACAC,MAAA,CAAAC,OAAA,MAAAjB,IAAA,CAAAe,MAAA,IAAAG,OAAA,WAAAC,IAAA;QAAA,IAAAC,KAAA,GAAAC,cAAA,CAAAF,IAAA;UAAAG,GAAA,GAAAF,KAAA;UAAAN,KAAA,GAAAM,KAAA;QACAhB,OAAA,CAAAC,GAAA,IAAAkB,MAAA,CAAAD,GAAA,QAAAC,MAAA,CAAAT,KAAA;MAAA;;MAEA;;MAEA;MACA;MACA;MACA;MACA;MACA;;MAEAV,OAAA,CAAAC,GAAA,CAAAO,KAAA;IACA;IACAY,cAAA,WAAAA,eAAA;MACApB,OAAA,CAAAC,GAAA,MAAAL,IAA